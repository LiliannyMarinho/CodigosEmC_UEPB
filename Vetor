#include <stdio.h>
#include <stdlib.h>

int main ()
{
  int A[10];
  int B[10];
  int i;

  for(i=0;i<10;i++)
    {
      printf("Insira o %dÂº valor: ",i+1);
      scanf("%d",&A[i]);
    }

  for(i=0;i<10;i++)
    {
      B[i] = A[i] * 10;
    }

  printf("\nVetor A:\n");
  for(i=0;i<10;i++)
    {
      printf("%d\n",A[i]);
    }
  printf("\n");
  
  printf("Vetor B:\n");
  for(i=0;i<10;i++)
    {
      printf("%d\n",B[i]);
    }

return 0;
}
